{
    "name": "citricguy/postmark-webhooks-laravel",
    "description": "Allow your Laravel project to listen for webhooks sent from PostmarkApp.",
    "type": "laravel-package",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Citricguy\\PostmarkWebhooks\\": "src/"
        }
    },
"autoload-dev": {
        "psr-4": {
            "Citricguy\\PostmarkWebhooks\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Citricguy\\PostmarkWebhooks\\PostmarkWebhooksServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Josh Sommers",
            "email": "citricguy@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}