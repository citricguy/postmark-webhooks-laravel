{
    "name": "citricguy/postmark-webhooks-laravel",
    "description": "Allow your Laravel project to listen for webhooks sent from PostmarkApp.",
    "type": "laravel-package",
    "require": {
        "php": "^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^1.20|^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Citricguy\\PostmarkWebhooks\\": "src/"
        }
    },
"autoload-dev": {
        "psr-4": {
            "Citricguy\\PostmarkWebhooks\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Citricguy\\PostmarkWebhooks\\PostmarkWebhooksServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Josh Sommers",
            "email": "citricguy@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
