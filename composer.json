{
    "name": "citricguy/postmark-webhooks-laravel",
    "description": "Allow your Laravel project to listen for webhooks sent from PostmarkApp.",
    "type": "laravel-package",
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "phpstan/phpstan": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Citricguy\\PostmarkWebhooks\\": "src/"
        }
    },
"autoload-dev": {
        "psr-4": {
            "Citricguy\\PostmarkWebhooks\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Citricguy\\PostmarkWebhooks\\PostmarkWebhooksServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Josh Sommers",
            "email": "citricguy@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest",
        "test:lint": "vendor/bin/pint --test",
        "test:analyse": "vendor/bin/phpstan analyse --memory-limit=2G",
        "lint": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse --memory-limit=2G"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
